<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.UserController">
   <children>
      <Text layoutX="38.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Write down your issue.">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextArea fx:id="textArea" layoutX="38.0" layoutY="61.0" prefHeight="278.0" prefWidth="521.0" />
      <Button fx:id="sendMessage" layoutX="492.0" layoutY="361.0" mnemonicParsing="false" onAction="#sendMessage" text="Send" />
      <Button fx:id="backToUser" layoutX="398.0" layoutY="361.0" mnemonicParsing="false" onAction="#backToUser" text="back to user" />
   </children>
</AnchorPane>
